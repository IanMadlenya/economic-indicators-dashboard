{
  "parser"  : "babel-eslint",
  "extends" : [
    "airbnb"
  ],
  "plugins": [
    "babel"
  ],
  "env"     : {
    "browser" : true
  },
  "globals" : {
    "Action"       : false,
    "__DEV__"      : false,
    "__PROD__"     : false,
    "__DEBUG__"    : false,
    "__BASENAME__" : false
  },
  "rules": {
    "semi" : [2, "never"],
    "max-len": [2, 120, 2],
    "generator-star-spacing": 0,
    "babel/generator-star-spacing": 1,
    "comma-dangle": [2, "never"],
    "arrow-body-style": "off",
    "jsx-quotes": ["error", "prefer-single"],
    "no-prototype-builtins": "off",
    "react/jsx-filename-extension" : "off",
    "no-console": "off",
  }
}
